{
  "$schema": "http://json-schema.org/schema",
  "name": "nx-autotag-libs",
  "version": "0.0.1",
  "generators": {
    "nx-autotag-libs": {
      "factory": "./src/generators/nx-autotag-libs/generator",
      "schema": "./src/generators/nx-autotag-libs/schema.json",
      "description": "nx-autotag-libs generator"
    },
    "ng-tagged-lib": {
      "factory": "./src/generators/ng-tagged-lib/generator",
      "schema": "./src/generators/ng-tagged-lib/schema.json",
      "description": "angular library automatically tagged by directory or name + given tag."
    },
    "workspace-tagged-lib": {
      "factory": "./src/generators/workspace-tagged-lib/generator",
      "schema": "./src/generators/workspace-tagged-lib/schema.json",
      "description": "workspace library automatically tagged by directory or name + given tag."
    }
  }
}
